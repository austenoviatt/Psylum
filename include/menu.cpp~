//Implementation program to test console background, color and ascii art

#include <iostream>

using namespace std;

int main()
{
   
}

class Program {
   static void Main(string[] args) {
      Console.BackgroundColor = ConsoleColor.Grey;
      Console.Clear();
      Console.ForegroundColor = ConsoleColor.White;
      Console.Write("Press any key to continue");
      Console.ReadKey();
   }
}
